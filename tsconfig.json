{
	"compilerOptions": {
		"jsx": "react-jsx",
		"module": "commonjs",
		"noImplicitAny": false,
		"preserveConstEnums": true,
		"removeComments": true,
		"target": "es5",
		"sourceMap": true,
		"esModuleInterop": true,
		"downlevelIteration": true,
		"lib": ["es6", "dom", "es2017"]
	},
	"include": ["./src/**/**/*", "config/custom.d.ts"]
}
